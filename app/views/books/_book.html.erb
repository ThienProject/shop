<div id="<%= dom_id book %>">
 <div class="card" style="width: 18rem;">
    <% if book.image.attached? %>
    <p><%= image_tag book.image, class: 'card-img-top' %></p>
    <% end %>
    <div class="card-body">
      <h5 class="card-title"><%= book.title %></h5>
      <p class="card-text"><%= book.author %></p>
       <p>
        <%= book.publication_year %>
       </p>
    </div>
  </div>
</div>
